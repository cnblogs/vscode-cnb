{
    "name": "vscode-cnb",
    "displayName": "博客园Cnblogs客户端",
    "description": "A vscode extension used to write and publish blog post to cnblogs",
    "version": "0.0.1",
    "license": "LICENSE.txt",
    "preview": false,
    "publisher": "cnblogs",
    "repository": {
        "type": "github",
        "url": "https://github.com/cnblogs/vscode-cnb"
    },
    "keywords": [
        "cnblogs",
        "blog"
    ],
    "icon": "dist/assets/logo.png",
    "engines": {
        "vscode": "^1.62.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:**",
        "onView:cnblogs-authorize",
        "onView:cnblogs-account",
        "onView:cnblogs-posts-list",
        "onView:vscode-cnb-workspace",
        "onCommand:vscode-cnb.open-workspace",
        "onCommand:vscode-cnb.login",
        "onCommand:vscode-cnb.logout",
        "onCommand:vscode-cnb.ing-publish-selection"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "markdown.markdownItPlugins": true,
        "markdown.previewScripts": [
            "./dist/markdown.js"
        ],
        "markdown.previewStyles": [
            "./dist/assets/styles/highlight-code-lines.css"
        ],
        "icons": {
            "vscode-cnb-date": {
                "description": "date",
                "default": {
                    "fontPath": "dist/assets/icons.woff2",
                    "fontCharacter": "\\e697"
                }
            },
            "vscode-cnb-image-upload": {
                "description": "image upload",
                "default": {
                    "fontPath": "dist/assets/icons.woff2",
                    "fontCharacter": "\\e80e"
                }
            },
            "vscode-cnb-cloud-upload": {
                "description": "cloud upload",
                "default": {
                    "fontPath": "dist/assets/icons.woff2",
                    "fontCharacter": "\\e7d9"
                }
            },
            "vscode-cnb-icon-seek": {
                "description": "icon seek",
                "default": {
                    "fontPath": "dist/assets/icons.woff2",
                    "fontCharacter": "\\e662"
                }
            },
            "vscode-cnb-folders": {
                "description": "folders",
                "default": {
                    "fontPath": "dist/assets/icons.woff2",
                    "fontCharacter": "\\e74a"
                }
            },
            "vscode-cnb-folder-close": {
                "description": "folder close",
                "default": {
                    "fontPath": "dist/assets/icons.woff2",
                    "fontCharacter": "\\e65b"
                }
            },
            "vscode-cnb-posts-list-search": {
                "description": "posts list search",
                "default": {
                    "fontPath": "dist/assets/icons.woff2",
                    "fontCharacter": "\\e610"
                }
            },
            "vscode-cnb-posts-list-search-result-summary": {
                "description": "posts list search result summary",
                "default": {
                    "fontPath": "dist/assets/icons.woff2",
                    "fontCharacter": "\\e674"
                }
            },
            "vscode-cnb-first-page": {
                "description": "first page",
                "default": {
                    "fontPath": "dist/assets/icons.woff2",
                    "fontCharacter": "\\e87a"
                }
            }
        },
        "commands": [
            {
                "title": "登录到博客园",
                "enablement": "!vscode-cnb.isAuthorized",
                "command": "vscode-cnb.login"
            },
            {
                "command": "vscode-cnb.logout",
                "title": "登出",
                "icon": "$(log-out)",
                "category": "Cnblogs Account",
                "enablement": "vscode-cnb.isAuthorized"
            },
            {
                "command": "vscode-cnb.previous-posts-list",
                "title": "上一页",
                "icon": "$(chevron-left)",
                "enablement": "vscode-cnb.isAuthorized && !vscode-cnb.posts-list.refreshing && vscode-cnb.posts-list.hasPrevious",
                "category": "Cnblogs Posts List"
            },
            {
                "command": "vscode-cnb.seek-posts-list",
                "title": "跳页",
                "icon": "$(vscode-cnb-icon-seek)",
                "enablement": "vscode-cnb.isAuthorized && !vscode-cnb.posts-list.refreshing && vscode-cnb.posts-list.pageCount > 0",
                "category": "Cnblogs Posts List"
            },
            {
                "command": "vscode-cnb.next-posts-list",
                "title": "下一页",
                "icon": "$(chevron-right)",
                "enablement": "vscode-cnb.isAuthorized && !vscode-cnb.posts-list.refreshing && vscode-cnb.posts-list.hasNext",
                "category": "Cnblogs Posts List"
            },
            {
                "command": "vscode-cnb.refresh-posts-list",
                "title": "刷新",
                "icon": "$(refresh)",
                "enablement": "vscode-cnb.isAuthorized && !vscode-cnb.posts-list.refreshing",
                "category": "Cnblogs Posts List"
            },
            {
                "command": "vscode-cnb.save-post",
                "title": "保存博文",
                "icon": "$(cloud-upload)",
                "category": "Cnblogs Posts List",
                "enablement": "vscode-cnb.isAuthorized"
            },
            {
                "command": "vscode-cnb.delete-post",
                "title": "删除随笔(支持多选)",
                "icon": "$(trash)",
                "category": "Cnblogs Posts List",
                "enablement": "vscode-cnb.isAuthorized"
            },
            {
                "command": "vscode-cnb.create-local-draft",
                "title": "新建本地草稿",
                "icon": "$(new-file)",
                "category": "Cnblogs Local Draft"
            },
            {
                "command": "vscode-cnb.modify-post-settings",
                "title": "博文设置",
                "icon": "$(gear)",
                "category": "Cnblogs Posts List",
                "enablement": "vscode-cnb.isAuthorized"
            },
            {
                "command": "vscode-cnb.upload-image",
                "title": "上传图片到博客园",
                "category": "Cnblogs",
                "icon": "$(vscode-cnb-image-upload)",
                "enablement": "vscode-cnb.isAuthorized"
            },
            {
                "command": "vscode-cnb.upload-clipboard-image",
                "title": "上传剪贴板图片到博客园",
                "category": "Cnblogs",
                "enablement": "vscode-cnb.isAuthorized"
            },
            {
                "command": "vscode-cnb.upload-local-disk-image",
                "title": "上传本地图片到博客园",
                "category": "Cnblogs",
                "enablement": "vscode-cnb.isAuthorized"
            },
            {
                "command": "vscode-cnb.reveal-local-post-file-in-os",
                "title": "在文件资源管理器中打开",
                "category": "Cnblogs"
            },
            {
                "command": "vscode-cnb.save-post-file-to-cnblogs",
                "title": "保存到博客园",
                "icon": "$(vscode-cnb-cloud-upload)",
                "enablement": "vscode-cnb.isAuthorized",
                "category": "Cnblogs"
            },
            {
                "command": "vscode-cnb.pull-post-remote-updates",
                "title": "拉取远程更新",
                "category": "Cnblogs",
                "enablement": "vscode-cnb.isAuthorized",
                "icon": "$(cloud-download)"
            },
            {
                "command": "vscode-cnb.show-post-to-local-file-info",
                "title": "博客园关联博文",
                "enablement": "vscode-cnb.isAuthorized"
            },
            {
                "command": "vscode-cnb.new-post-category",
                "title": "新建博文分类",
                "icon": "$(add)",
                "category": "Cnblogs Post Categories Management",
                "enablement": "vscode-cnb.isAuthorized"
            },
            {
                "command": "vscode-cnb.delete-selected-post-categories",
                "title": "删除",
                "icon": "$(trash)",
                "category": "Cnblogs Post Categories Management",
                "enablement": "vscode-cnb.isAuthorized && !vscode-cnb.postCategoriesList.isRefreshing"
            },
            {
                "command": "vscode-cnb.refresh-post-categories-list",
                "title": "刷新",
                "icon": "$(refresh)",
                "category": "Cnblogs Post Categories Management",
                "enablement": "!vscode-cnb.postCategoriesList.isRefreshing"
            },
            {
                "command": "vscode-cnb.update-post-category",
                "title": "编辑",
                "icon": "$(edit)",
                "category": "Cnblogs Post Categories Management",
                "enablement": "!vscode-cnb.postCategoriesList.isRefreshing"
            },
            {
                "command": "vscode-cnb.delete-post-to-local-file-map",
                "title": "取消关联本地文件(支持多选)",
                "category": "Cnblogs Posts List"
            },
            {
                "command": "vscode-cnb.rename-post",
                "title": "重命名博文",
                "category": "Cnblogs Posts List",
                "enablement": "vscode-cnb.isAuthorized"
            },
            {
                "command": "vscode-cnb.open-post-in-blog-admin",
                "title": "在博客后台中编辑",
                "category": "Cnblogs",
                "enablement": "vscode-cnb.isAuthorized"
            },
            {
                "command": "vscode-cnb.open-workspace",
                "title": "打开工作空间",
                "category": "Cnblogs"
            },
            {
                "command": "vscode-cnb.view-post-online",
                "title": "在线查看博文",
                "category": "Cnblogs"
            },
            {
                "command": "vscode-cnb.export-post-to-pdf",
                "title": "导出pdf",
                "category": "Cnblogs",
                "enablement": "vscode-cnb.isAuthorized"
            },
            {
                "command": "vscode-cnb.extract-images",
                "title": "提取图片",
                "category": "Cnblogs",
                "enablement": "vscode-cnb.isAuthorized"
            },
            {
                "command": "vscode-cnb.search-posts",
                "title": "搜索博文",
                "category": "Cnblogs",
                "icon": "$(vscode-cnb-posts-list-search)",
                "enablement": "vscode-cnb.isAuthorized && !vscode-cnb.posts-list.refreshing"
            },
            {
                "command": "vscode-cnb.clear-posts-search-results",
                "title": "清除随笔搜索结果",
                "category": "Cnblogs",
                "icon": "$(clear-all)",
                "enablement": "vscode-cnb.isAuthorized && !vscode-cnb.posts-list.refreshing"
            },
            {
                "command": "vscode-cnb.refresh-posts-search-results",
                "title": "刷新随笔搜索结果",
                "category": "Cnblogs",
                "icon": "$(refresh)",
                "enablement": "vscode-cnb.isAuthorized && !vscode-cnb.posts-list-refreshing"
            },
            {
                "command": "vscode-cnb.ing.publish",
                "title": "发闪存",
                "category": "Cnblogs",
                "enablement": "vscode-cnb.isAuthorized && !vscode-cnb.ingsList.isRefreshing",
                "icon": "$(add)"
            },
            {
                "command": "vscode-cnb.ing.publish-selection",
                "title": "将选中内容发到闪存",
                "category": "Cnblogs",
                "enablement": "vscode-cnb.isAuthorized && editorHasSelection == true && isInDiffEditor == false && isInEmbeddedEditor == false"
            },
            {
                "command": "vscode-cnb.ings-list.refresh",
                "title": "刷新闪存列表",
                "category": "Cnblogs",
                "icon": "$(refresh)",
                "enablement": "vscode-cnb.isAuthorized && !vscode-cnb.ingsList.isRefreshing"
            },
            {
                "command": "vscode-cnb.ings-list.previous",
                "title": "上一页",
                "category": "Cnblogs",
                "icon": "$(chevron-left)",
                "enablement": "vscode-cnb.isAuthorized && !vscode-cnb.ingsList.isRefreshing && vscode-cnb.ingsList.pageIndex > 1"
            },
            {
                "command": "vscode-cnb.ings-list.next",
                "title": "下一页",
                "category": "Cnblogs",
                "icon": "$(chevron-right)",
                "enablement": "vscode-cnb.isAuthorized && !vscode-cnb.ingsList.isRefreshing && vscode-cnb.ingsList.pageIndex < 500"
            },
            {
                "command": "vscode-cnb.ings-list.first",
                "title": "第一页",
                "category": "Cnblogs",
                "icon": "$(vscode-cnb-first-page)",
                "enablement": "vscode-cnb.isAuthorized && !vscode-cnb.ingsList.isRefreshing && vscode-cnb.ingsList.pageIndex < 500"
            },
            {
                "command": "vscode-cnb.ings-list.select-type",
                "title": "选择闪存列表",
                "category": "Cnblogs",
                "icon": "$(list-filter)",
                "enablement": "vscode-cnb.isAuthorized && !vscode-cnb.ingsList.isRefreshing"
            },
            {
                "command": "vscode-cnb.ings-list.open-in-browser",
                "title": "在浏览器中打开闪存",
                "icon": "$(globe)",
                "enablement": "vscode-cnb.isAuthorized"
            },
            {
                "command": "vscode-cnb.copy-post-link",
                "title": "复制博文链接",
                "icon": "$(link)",
                "enablement": "vscode-cnb.isAuthorized"
            }
        ],
        "configuration": [
            {
                "title": "vscode-cnb",
                "properties": {
                    "cnblogsClientForVSCode.workspace": {
                        "order": 0,
                        "default": "~/Documents/Cnblogs",
                        "scope": "application",
                        "type": "string",
                        "editPresentation": "singlelineText",
                        "markdownDescription": "存放博文的文件夹, 默认为`~/Documents/Cnblogs`, 当检测到vscode处于此工作空间时, 可以vscode-cnb插件编辑已有博文, 也可以将本地markdown文件保存到博客园"
                    },
                    "cnblogsClientForVSCode.windows.chromiumPath": {
                        "order": 1,
                        "default": "C:\\Program Files (x86)\\Microsoft\\Edge\\Application\\msedge.exe",
                        "scope": "application",
                        "type": "string",
                        "editPresentation": "singlelineText",
                        "markdownDescription": "Chromium可执行文件路径, 用于进行pdf导出等操作"
                    },
                    "cnblogsClientForVSCode.macos.chromiumPath": {
                        "order": 2,
                        "default": "/Applications/Google Chrome.app/Contents/MacOS/Google Chrome",
                        "scope": "application",
                        "type": "string",
                        "editPresentation": "singlelineText",
                        "markdownDescription": "Chromium可执行文件路径, 用于进行pdf导出等操作"
                    },
                    "cnblogsClientForVSCode.createLocalPostFileWithCategory": {
                        "order": 3,
                        "default": true,
                        "scope": "application",
                        "type": "boolean",
                        "markdownDescription": "创建本地博文时, 是否根据博文分类保存到对应的文件夹中"
                    },
                    "cnblogsClientForVSCode.automaticallyExtractImages": {
                        "order": 4,
                        "default": "---",
                        "scope": "application",
                        "enum": [
                            "---",
                            "local",
                            "web",
                            "all"
                        ],
                        "enumItemLabels": [
                            "不自动提取图片",
                            "自动提取本地图片",
                            "自动提取网络图片",
                            "自动提取全部图片"
                        ],
                        "editPresentation": "singlelineText",
                        "markdownDescription": "提取图片, 配置保存到博客园时要自动提取上传到博客园的图片"
                    },
                    "cnblogsClientForVSCode.pageSize.postsList": {
                        "order": 5,
                        "default": 30,
                        "scope": "application",
                        "editPresentation": "singlelineText",
                        "markdownDescription": "随笔列表分页大小, 自定义随笔列表每页展示数量",
                        "type": "integer",
                        "maximum": 100,
                        "enum": [
                            20,
                            30,
                            40,
                            50,
                            60,
                            70,
                            80,
                            90,
                            100
                        ]
                    },
                    "cnblogsClientForVSCode.markdown.enableEnhancement": {
                        "order": 6,
                        "markdownDescription": "启用博客园markdown语法扩展",
                        "default": true,
                        "type": "boolean",
                        "scope": "application"
                    },
                    "cnblogsClientForVSCode.markdown.enableFenceQuote": {
                        "order": 7,
                        "type": "boolean",
                        "scope": "application",
                        "default": true,
                        "markdownDescription": "博客园markdown语法扩展: 三箭头(`<<<`)栅栏引用\n\n&nbsp;\n\n使用示例:\n\n```markdown\n\n<<<\n一段引用文字\n\n一段引用文字\n<<<\n\n```"
                    },
                    "cnblogsClientForVSCode.markdown.enableHighlightCodeLines": {
                        "order": 8,
                        "type": "boolean",
                        "scope": "application",
                        "default": true,
                        "editPresentation": "singlelineText",
                        "markdownDescription": "博客园markdown语法扩展: 代码块指定行高亮\n\n&nbsp;\n\n使用示例:\n\n```markdown\n\n```typescript {1, 3-4}\nconsole.log(1)\nconsole.log(2)\nconsole.log(3)\nconsole.log(4)\nconsole.log(5)\n```\n\n```"
                    },
                    "cnblogsClientForVSCode.ing.enablePublishSelectionToIng": {
                        "order": 9,
                        "type": "boolean",
                        "scope": "application",
                        "default": false,
                        "markdownDescription": "是否允许将选中内容发到闪存\n\n启用后, 编辑上下文菜单将包含\"将选中内容发到闪存\""
                    }
                }
            }
        ],
        "views": {
            "explorer": [
                {
                    "id": "cnblogs-posts-list",
                    "name": "博客园 - 随笔列表",
                    "when": "vscode-cnb.isAuthorized && vscode-cnb.isTargetWorkspace"
                }
            ],
            "cnblogs": [
                {
                    "id": "cnblogs-authorize",
                    "name": "登录/授权",
                    "when": "!vscode-cnb.isAuthorized",
                    "visibility": "visible"
                },
                {
                    "id": "vscode-cnb-workspace",
                    "name": "工作空间",
                    "when": "vscode-cnb.isAuthorized",
                    "visibility": "collapsed"
                },
                {
                    "id": "cnblogs-posts-list-another",
                    "name": "博客园 - 随笔列表",
                    "when": "vscode-cnb.isAuthorized",
                    "visibility": "collapsed"
                },
                {
                    "id": "cnblogs-post-categories-list",
                    "name": "分类列表",
                    "when": "vscode-cnb.isAuthorized",
                    "visibility": "collapsed"
                },
                {
                    "id": "cnblogs-account",
                    "name": "账户信息",
                    "when": "vscode-cnb.isAuthorized",
                    "visibility": "collapsed"
                },
                {
                    "id": "cnblogs-navigation",
                    "name": "博客园导航",
                    "when": "vscode-cnb.isAuthorized",
                    "visibility": "collapsed"
                },
                {
                    "id": "vscode-cnb.ings-list-webview",
                    "type": "webview",
                    "name": "闪存",
                    "when": "vscode-cnb.isAuthorized",
                    "visibility": "collapsed"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "vscode-cnb.logout",
                    "when": "view == cnblogs-account",
                    "group": "navigation"
                },
                {
                    "command": "vscode-cnb.previous-posts-list",
                    "when": "view == cnblogs-posts-list || view == cnblogs-posts-list-another",
                    "group": "navigation@1"
                },
                {
                    "command": "vscode-cnb.next-posts-list",
                    "when": "view == cnblogs-posts-list || view == cnblogs-posts-list-another",
                    "group": "navigation@2"
                },
                {
                    "command": "vscode-cnb.seek-posts-list",
                    "when": "view == cnblogs-posts-list || view == cnblogs-posts-list-another",
                    "group": "navigation@3"
                },
                {
                    "command": "vscode-cnb.refresh-posts-list",
                    "when": "view == cnblogs-posts-list || view == cnblogs-posts-list-another",
                    "group": "navigation@4"
                },
                {
                    "command": "vscode-cnb.search-posts",
                    "when": "view == cnblogs-posts-list || view == cnblogs-posts-list-another",
                    "group": "navigation@5"
                },
                {
                    "command": "vscode-cnb.create-local-draft",
                    "group": "navigation@6",
                    "when": "view == cnblogs-posts-list || view == cnblogs-posts-list-another"
                },
                {
                    "command": "vscode-cnb.new-post-category",
                    "group": "navigation@1",
                    "when": "view == cnblogs-post-categories-list"
                },
                {
                    "command": "vscode-cnb.refresh-post-categories-list",
                    "when": "view == cnblogs-post-categories-list",
                    "group": "navigation@2"
                },
                {
                    "command": "vscode-cnb.ings-list.open-in-browser",
                    "when": "view == vscode-cnb.ings-list-webview",
                    "group": "navigation@0"
                },
                {
                    "command": "vscode-cnb.ings-list.first",
                    "when": "view == vscode-cnb.ings-list-webview && vscode-cnb.isAuthorized && !vscode-cnb.ingsList.isRefreshing && vscode-cnb.ingsList.pageIndex > 2",
                    "group": "navigation@1"
                },
                {
                    "command": "vscode-cnb.ings-list.previous",
                    "when": "view == vscode-cnb.ings-list-webview",
                    "group": "navigation@2"
                },
                {
                    "command": "vscode-cnb.ings-list.next",
                    "when": "view == vscode-cnb.ings-list-webview",
                    "group": "navigation@3"
                },
                {
                    "command": "vscode-cnb.ings-list.select-type",
                    "when": "view == vscode-cnb.ings-list-webview",
                    "group": "navigation@4"
                },
                {
                    "command": "vscode-cnb.ings-list.refresh",
                    "when": "view == vscode-cnb.ings-list-webview",
                    "group": "navigation@5"
                },
                {
                    "command": "vscode-cnb.ing.publish",
                    "when": "view == vscode-cnb.ings-list-webview",
                    "group": "navigation@6"
                }
            ],
            "commandPalette": [
                {
                    "command": "vscode-cnb.reveal-local-post-file-in-os",
                    "when": "false"
                },
                {
                    "command": "vscode-cnb.save-post",
                    "when": "false"
                },
                {
                    "command": "vscode-cnb.modify-post-settings",
                    "when": "false"
                },
                {
                    "command": "vscode-cnb.delete-post",
                    "when": "false"
                },
                {
                    "command": "vscode-cnb.save-post-file-to-cnblogs",
                    "when": "true"
                },
                {
                    "command": "vscode-cnb.logout",
                    "when": "false"
                },
                {
                    "command": "vscode-cnb.show-post-to-local-file-info",
                    "when": "false"
                },
                {
                    "command": "vscode-cnb.new-post-category",
                    "when": "false"
                },
                {
