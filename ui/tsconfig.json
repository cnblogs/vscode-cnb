{
    "compilerOptions": {
        "module": "es2015",
        "target": "es5",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "outDir": "./dist",
        "allowSyntheticDefaultImports": true,
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "baseUrl": ".",
        "paths": {
            "@models/*": ["../src/models/*"],
            "@/models/*": ["../src/models/*"]
        },
        "lib": ["DOM", "ES2020"],
        "strictBindCallApply": true
    },
    "include": ["./**/*.ts", "./**/*.tsx", "./**/*.jsx", "./**/*.js", "*.mjs", "../src/models/*.ts"],
    "exclude": ["./dist"]
}
