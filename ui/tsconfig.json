{
    "compilerOptions": {
        "module": "ES2020",
        "target": "ES2020",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "outDir": "./dist",
        "allowSyntheticDefaultImports": true,
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "baseUrl": ".",
        "paths": {
            "@/*": ["../src/*"]
        },
        "lib": ["DOM", "ES2020"],
        "strictBindCallApply": true,
        "rootDir": "../"
    },
    "include": ["./**/*.ts", "./**/*.tsx", "./**/*.jsx", "./**/*.js", "*.mjs", "../src/model/*.ts"],
    "exclude": ["./dist"]
}
